{
  "globalStyle": {
    // 导航背景色
    // (HexColor)
    // alipay->titleBarColor
    "navigationBarBackgroundColor": "#FFF",
    // 导航文字色
    // (black|white)
    // alipay->根据navigationBarBackgroundColor自适应
    "navigationBarTextStyle": "black",
    // 导航标题
    // (string)
    // alipay->defaultTitle
    "navigationBarTitleText": "uni-vant",
    // 导航栏样式
    // (default|custom)
    // [weixin, baidu, h5, app]
    "navigationStyle": "default",
    // 窗口背景色
    // (HexColor)
    // weixin->下拉窗口,alipay->整个页面
    "backgroundColor": "#FFFFFF",
    // 下拉loading样式
    // (dark|light)
    // [weixin] alipay未实现
    "backgroundTextStyle": "dark",
    // 下拉刷新
    // (boolean)
    // alipay->pullRefresh需配合allowsBounceVertical生效,开发工具无效
    "enablePullDownRefresh": true,
    // 上拉触底距离
    // (number)
    "onReachBottomDistance": 50,
    // IOS顶部窗口bounce回弹区域背景色
    // (HexColor) todo 需要苹果手机看看具体
    "backgroundColorTop": "#FFFFFF",
    // IOS底部窗口bounce回弹区域背景色
    // (HexColor)
    "backgroundColorBottom": "#FFFFFF",
    // 导航图片
    // (string)
    // [alipay, h5, app]
    "titleImage": "",
    // 导航栏点击穿透
    // (YES|NO)
    // [alipay, h5, app]
    "titlePenetrate": "NO",
    // 导航栏透明
    // (always|auto|none)
    // [alipay, h5]
    "transparentTitle": "none",
    // 横屏配置
    // (auto|portrait|landscape)
    // [weixin, app]
    "pageOrientation": "portrait",
    // 窗口显示的动画效果
    // (slide-in-right|slide-in-left|slide-in-top|slide-in-bottom|pop-in|fade-in|zoom-out|zoom-fade-out|none)
    // [app]
    "animationType": "pop-in",
    // 窗口显示动画的持续时间
    // (number)
    "animationDuration": "300",
    "app-plus": {},
    "h5": {
      // 导航栏
      // (object)
      "titleNView": {
        // 标题背景
        // (string)
        "backgroundColor": "#FFF",
        "buttons": [
          {
            "type": "back"
          }
        ],
        // 标题文字颜色
        // (string)
        "titleColor": "#000",
        // 标题文字内容
        // (string)
        "titleText": "uni-vant-h5",
        // 标题文字字体大小
        // (string)
        "titleSize": "16px",
        // todo 这个设置透明后transparent后,整个标题都看不见啦
        // 航栏样式
        // (default|transparent)
        // 默认隐藏,向下滑动时显示
        "type": "default"
      },
      "pullToRefresh": {}
    },
    "mp-alipay": {
      "allowsBounceVertical": "YES", // 向下拉拽(YES|NO)
      "backgroundImageUrl": "https://img6.xiujiadian.com/temp/ranwawa/20200227.png", // 下拉背景图(string) todo 测试未生效
      "backgroundImageColor": "#ff00ee" // 下拉背景图底色(HexColor) todo 测试未生效
    },
    "mp-weixin": {
    },
    "mp-baidu": {
    },
    "mp-toutiao": {
    },
    "mp-qq": {
    },
    "usingComponents": {
    }
  },
  // 自动引入注册组件
  "easycom": {
    // 自动扫描
    // (boolean)
    // 符合components/组件名称/组件名称.vue目录结构的组件
    "autoscan": true,
    // 自定义组件匹配规则
    // (object)
    "custom": {}
  },
  // 启动模式配置
  // (object)
  // 仅开发模式生效
  "condition": {
    // *当前激活的模式
    // (number)
    // list节点的索引值
    "current": 0,
    // *启动模式列表
    // (array)
    "list": [
      {
        // *启动模式名称
        // (string)
        "name": "button",
        // *启动页面路径
        // (string)
        "path": "pages/button",
        // 启动参数
        // (string)
        "query": ""
      }
    ]
  },
  "pages": [
    {
      "path": "pages/index",
      "style": {
        "navigationBarTitleText": "uni-vant"
      }
    },
    {
      "path": "pages/icon",
      "style": {
        "navigationBarTitleText": "icon"
      }
    },
    {
      "path": "pages/button",
      "style": {
        "navigationBarTitleText": "button"
      }
    },
    {
      "path": "pages/uploader",
      "style": {
        "navigationBarTitleText": "uploader"
      }
    },
    {
      "path": "pages/swiper-dot",
      "style": {
        "navigationBarTitleText": "swiper-dot"
      }
    },
    {
      "path": "pages/loading",
      "style": {
        "navigationBarTitleText": "loading"
      }
    },
    {
      "path": "pages/sticky",
      "style": {
        "navigationBarTitleText": "sticky"
      }
    },
    {
      "path": "pages/field",
      "style": {
        "navigationBarTitleText": "field"
      }
    },
    {
      "path": "pages/transition",
      "style": {
        "navigationBarTitleText": "transition"
      }
    },
    {
      "path": "pages/overlay",
      "style": {
        "navigationBarTitleText": "overlay"
      }
    },
    {
      "path": "pages/test",
      "style": {
        "navigationBarTitleText": "test"
      }
    },
    {
      "path": "pages/cell",
      "style": {
        "navigationBarTitleText": "cell"
      }
    },
    {
      "path": "pages/popup",
      "style": {
        "navigationBarTitleText": "popup"
      }
    },
    {
      "path": "pages/result",
      "style": {
        "navigationBarTitleText": "result"
      }
    },
    {
      "path": "pages/skeleton",
      "style": {
        "navigationBarTitleText": "skeleton"
      }
    },
    {
      "path": "pages/tag",
      "style": {
        "navigationBarTitleText": "tag"
      }
    },{
      "path": "pages/nav-bar",
      "style": {
        "navigationBarTitleText": "nav-bar"
      }
    }
  ]
}
